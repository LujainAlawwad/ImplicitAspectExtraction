    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "from tqdm import tqdm\n",
        "\n",
        "def normalize_aspect_ar(aspect: str) -> str:\n",
        "    asp = str(aspect).strip()\n",
        "    asp = re.sub(r\"^\\W+|\\W+$\", \"\", asp)\n",
        "    return asp\n",
        "\n",
        "def filter_to_sentence_ar(aspects, original_sentence):\n",
        "    sent = original_sentence\n",
        "    return [a for a in aspects if a in sent]\n",
        "\n",
        "\n",
        "def iterative_residual_decoding_with_history_ar(sentence, max_rounds=5):\n",
        "    \"\"\"\n",
        "    Arabic version of IG with per-round snapshots.\n",
        "    \"\"\"\n",
        "    original_sentence = sentence\n",
        "    remaining_sentence = sentence\n",
        "    all_aspects = []\n",
        "    seen_norm = set()\n",
        "    per_round_outputs = []\n",
        "\n",
        "    for round_id in range(max_rounds):\n",
        "        aspects = extract_aspect(remaining_sentence)  # Arabic extractor\n",
        "        if not aspects:\n",
        "            break\n",
        "\n",
        "        new_aspects = [a.strip() for a in aspects if a and a.strip()]\n",
        "        if not new_aspects or all(a.lower() == \"none\" for a in new_aspects):\n",
        "            break\n",
        "\n",
        "        new_aspects = filter_to_sentence_ar(new_aspects, original_sentence)\n",
        "\n",
        "        for asp in new_aspects:\n",
        "            norm = normalize_aspect_ar(asp)\n",
        "            if norm not in seen_norm:\n",
        "                seen_norm.add(norm)\n",
        "                all_aspects.append(asp)\n",
        "\n",
        "            # remove exact aspect from remaining sentence\n",
        "            pattern = r\"\\b{}\\b\".format(re.escape(asp))\n",
        "            remaining_sentence = re.sub(pattern, \"\", remaining_sentence).strip()\n",
        "\n",
        "        # snapshot\n",
        "        final_aspects = [a for a in all_aspects if a.lower() != \"none\"]\n",
        "        per_round_outputs.append(final_aspects.copy())\n",
        "\n",
        "        if not remaining_sentence:\n",
        "            break\n",
        "\n",
        "    return per_round_outputs\n"
      ],
      "metadata": {
        "id": "n0FSBtQ92kyD"
      },
      "execution_count": null,
      "outputs": []
    },
