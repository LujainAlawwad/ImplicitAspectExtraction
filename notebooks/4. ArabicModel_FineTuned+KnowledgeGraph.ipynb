        "\n",
        "# Copy to avoid mutating original\n",
        "df_plot = summary_df.copy()\n",
        "\n",
        "# --- extract k value ---\n",
        "def extract_k(setting):\n",
        "    if setting == \"pred_orig_pp\":\n",
        "        return 0\n",
        "    m = re.search(r\"k_(\\d+)Preds\", setting)\n",
        "    return int(m.group(1)) if m else None\n",
        "\n",
        "df_plot[\"k\"] = df_plot[\"Setting\"].apply(extract_k)\n",
        "\n",
        "# Remove duplicate k=0 (keep pred_orig_pp only)\n",
        "df_plot = df_plot.sort_values(\"Setting\")\n",
        "df_plot = df_plot.drop_duplicates(subset=[\"k\"], keep=\"first\")\n",
        "\n",
        "# Sort by k\n",
        "df_plot = df_plot.sort_values(\"k\")\n",
        "\n",
        "df_plot\n",
        "plt.figure(figsize=(7, 5))\n",
        "\n",
        "x = df_plot[\"k\"]\n",
