        "\n",
        "def infer_implicit_from_kg_best(sentence, kg, domain_embs, domain_conf_threshold=0.1):\n",
        "    lonely = find_lonely_adjectives_dbg(sentence)\n",
        "    if not lonely:\n",
        "        return []\n",
        "\n",
        "    dom, dom_score = infer_domain_with_centroid_dbg(sentence, domain_embs)\n",
        "    if not dom or dom_score < domain_conf_threshold:\n",
        "        return []\n",
        "\n",
        "    implicit = []\n",
        "    for adj in lonely:\n",
        "        aspects = kg.get_aspects(adj)\n",
        "        # filter aspects by domain\n",
        "        valid_aspects = []\n",
        "        for asp in aspects:\n",
        "            ents = kg.get_entities(asp)\n",
        "            doms = {d.lower() for e in ents for d in kg.get_domains(e)}\n",
        "            if dom.lower() in doms:\n",
        "                valid_aspects.append(asp)\n",
        "\n",
        "        # best = choose_best_aspect_for_adj(adj, valid_aspects, dom, domain_embs, kg, top_k=1)\n",
        "        best = choose_best_aspect_for_adj(adj, valid_aspects, top_k=1)\n",
        "        implicit.extend(best)\n",
        "\n",
        "    # Deduplicate\n",
        "    seen, result = set(), []\n",
        "    for a in implicit:\n",
        "        if a.lower() not in seen:\n",
        "            seen.add(a.lower())\n",
        "            result.append(a)\n",
