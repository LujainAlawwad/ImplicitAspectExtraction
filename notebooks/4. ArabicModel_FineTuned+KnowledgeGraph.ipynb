        "# -*- coding: utf-8 -*-\n",
        "import re\n",
        "from typing import List, Optional\n",
        "\n",
        "# ========= Normalization (single source of truth) =========\n",
        "_DIACRITICS = re.compile(r'[\\u0617-\\u061A\\u064B-\\u0652]')\n",
        "_PREFIXES  = re.compile(r'^(?:ﾘｧﾙлﾙ|ﾙ−ﾘｨ|ﾙлﾙ)+')\n",
        "_SUFFIXES  = re.compile(r'(?:ﾙﾙﾘｧ|ﾙﾙﾘｧ|ﾙﾘｧ|ﾙﾙ|ﾙﾘｧ|ﾙﾙ|ﾙﾙ|ﾙﾙ|ﾙ|ﾙ掛ﾘｩ|ﾘｧﾘｪ)$')\n",
        "\n",
        "def normalize_token(tok: str) -> str:\n",
        "    t = str(tok)\n",
        "    t = (t.replace(\"ﾘ｣\", \"ﾘｧ\").replace(\"ﾘ･\", \"ﾘｧ\").replace(\"ﾘ｢\", \"ﾘｧ\")\n",
        "           .replace(\"ﾙ噂", \"ﾙ浬").replace(\"ﾘｩ\", \"ﾙ\")\n",
        "           .replace(\"ﾘ､\", \"ﾘ｡\").replace(\"ﾘｦ\", \"ﾘ｡\"))\n",
        "    t = _DIACRITICS.sub(\"\", t).replace(\"ﾙ\", \"\")\n",
        "    t = _PREFIXES.sub(\"\", t)\n",
        "    t = _SUFFIXES.sub(\"\", t)\n",
        "    return t.strip()\n",
        "\n",
        "def normalize_phrase(phrase: str) -> str:\n",
        "    toks = [w for w in re.split(r\"\\s+\", str(phrase).strip()) if w]\n",
        "    return \" \".join(normalize_token(w) for w in toks)\n",
        "\n",
        "# ========= Parse model string -> list, with normalized-key dedup =========\n",
        "_SEP_PAT = re.compile(\n",
        "    r\"\\s*(?:\\[sep\\]|\\[SEP\\]|ﾘ芸,|;|ﾘ斈\\||\\n|\\t|\\/|\\\\|:|\\s{2,})\\s*\", re.IGNORECASE\n",
        ")\n",
        "_EDGE_PUNCT = re.compile(r\"^[\\s\\-\\窶貼\窶能\ﾂｷ\\窶｢\\*\\.]+|[\\s\\-\\窶貼\窶能\ﾂｷ\\窶｢\\*\\.]+$\")\n",
        "\n",
        "def parse_aspect_string(s) -> List[str]:\n",
        "    if s is None:\n",
        "        return []\n",
        "    if isinstance(s, list):\n",
        "        items = s\n",
        "    else:\n",
        "        s = str(s).strip().strip(\"[](){}\").replace(\"'\", \"\").replace('\"', \"\")\n",
        "        items = _SEP_PAT.split(s)\n",
        "\n",
        "    cleaned = []\n",
        "    for t in items:\n",
        "        t = _EDGE_PUNCT.sub(\"\", str(t).strip())\n",
        "        if t:\n",
        "            cleaned.append(t)\n",
        "\n",
        "    # dedup by normalized key, keep first surface\n",
        "    seen = set()\n",
        "    out = []\n",
        "    for w in cleaned:\n",
        "        key = normalize_phrase(w)\n",
        "        if key and key not in seen:\n",
        "            seen.add(key)\n",
        "            out.append(w)\n",
        "    return out\n",
        "\n",
        "# ========= CAMeL-only adjective removal =========\n",
        "class CamelAdjRemover:\n",
        "    \"\"\"Remove adjectives using CAMeL Tools POS tags (no heuristic fallback).\"\"\"\n",
        "    def __init__(self):\n",
        "        from camel_tools.tokenizers.word import simple_word_tokenize\n",
        "        from camel_tools.disambig.mle import MLEDisambiguator\n",
        "        self.tok = simple_word_tokenize\n",
        "        self.mle = MLEDisambiguator.pretrained()\n",
        "\n",
        "    def strip_adjectives(self, phrase: str) -> str:\n",
        "        tokens = self.tok(str(phrase))\n",
        "        if not tokens:\n",
        "            return \"\"\n",
        "        disamb = self.mle.disambiguate(tokens)\n",
        "        kept = []\n",
        "        for t, d in zip(tokens, disamb):\n",
        "            pos = None\n",
        "            if d.analyses:\n",
        "                a = d.analyses[0]\n",
        "                pos = (getattr(a, \"pos\", None) or getattr(a, \"pos_tag\", \"\") or \"\").lower()\n",
        "            # Keep everything that is NOT adjective; drop adjectives only\n",
        "            if not (pos.startswith(\"adj\") or pos == \"a\"):\n",
        "                kept.append(t)\n",
        "        # If everything was dropped (unlikely), fail-open: keep original phrase\n",
        "        return \" \".join(kept).strip() if kept else str(phrase).strip()\n",
        "\n",
        "# Lazily create a shared CAMeL object (avoid reloading per row)\n",
        "_CAMEL_SINGLETON: Optional[CamelAdjRemover] = None\n",
        "def get_camel() -> CamelAdjRemover:\n",
        "    global _CAMEL_SINGLETON\n",
        "    if _CAMEL_SINGLETON is None:\n",
        "        _CAMEL_SINGLETON = CamelAdjRemover()\n",
        "    return _CAMEL_SINGLETON\n",
        "\n",
        "# ========= Post-process entrypoint (CAMeL only) =========\n",
        "def post_process(raw_pred: str,\n",
        "                 camel_obj: Optional[CamelAdjRemover] = None) -> List[str]:\n",
        "    \"\"\"\n",
        "    1) parse -> list\n",
        "    2) remove adjectives via CAMeL POS ONLY\n",
        "    3) dedup by normalized form\n",
        "    If CAMeL is unavailable at runtime, no stripping is applied.\n",
        "    \"\"\"\n",
        "    aspects = parse_aspect_string(raw_pred)\n",
        "\n",
        "    stripped = []\n",
        "    try:\n",
        "        camel = camel_obj or get_camel()\n",
        "        for a in aspects:\n",
        "            stripped.append(camel.strip_adjectives(a))\n",
        "    except Exception:\n",
        "        # CAMeL not available -> keep original phrases (do NOT heuristic)\n",
        "        stripped = aspects\n",
        "\n",
        "    # drop empties and dedup by normalized key\n",
        "    seen = set()\n",
        "    final = []\n",
        "    for a in stripped:\n",
        "        a = a.strip()\n",
        "        if not a:\n",
        "            continue\n",
        "        key = normalize_phrase(a)\n",
        "        if key and key not in seen:\n",
        "            seen.add(key)\n",
        "            final.append(a)\n",
        "    return final\n"
      ]
    },
    {
