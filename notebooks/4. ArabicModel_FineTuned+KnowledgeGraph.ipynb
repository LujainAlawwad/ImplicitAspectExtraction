        "import matplotlib.pyplot as plt\n",
        "\n",
        "def plot_sensitivity(df, xcol, pcol, rcol, fcol, xlabel, title, save_prefix=None):\n",
        "    x = df[xcol]\n",
        "\n",
        "    plt.figure(figsize=(7, 5))\n",
        "    plt.plot(x, df[pcol], marker=\"o\", label=\"Precision\")\n",
        "    plt.plot(x, df[rcol], marker=\"s\", label=\"Recall\")\n",
        "    plt.plot(x, df[fcol], marker=\"^\", label=\"Micro-F1\")\n",
        "\n",
        "    plt.xlabel(xlabel, fontsize=12)\n",
        "    plt.ylabel(\"Score\", fontsize=12)\n",
        "    plt.title(title, fontsize=14)\n",
        "\n",
        "    plt.xticks(x, fontsize=10)\n",
        "    plt.yticks(fontsize=10)\n",
        "\n",
        "    plt.grid(True, linestyle=\"--\", alpha=0.6)\n",
        "    plt.legend(fontsize=11)\n",
        "    plt.tight_layout()\n",
        "\n",
        "    if save_prefix:\n",
        "        plt.savefig(f\"{save_prefix}.pdf\", dpi=300, bbox_inches=\"tight\")\n",
        "        plt.savefig(f\"{save_prefix}.png\", dpi=300, bbox_inches=\"tight\")\n",
        "\n",
        "    plt.show()\n"
