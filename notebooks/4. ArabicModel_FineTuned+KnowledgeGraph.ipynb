        "import pandas as pd\n",
        "\n",
        "# -----------------------------\n",
        "# Evaluate paraphrase-k settings\n",
        "# -----------------------------\n",
        "K_COLS = [\"pred_orig_pp\",\"k_0Preds_pp\", \"k_2Preds_pp\", \"k_4Preds_pp\", \"k_6Preds_pp\", \"k_8Preds_pp\", \"k_10Preds_pp\"]\n",
        "\n",
        "# Set this to your gold column name:\n",
        "# From your screenshot it looks like: \"aspect\"\n",
        "# If your English file uses \"aspectTerms\" then change accordingly.\n",
        "GOLD_COL = \"aspect\"   # <-- change if needed\n",
        "\n",
        "rows = []\n",
        "for pred_col in K_COLS:\n",
        "    metrics = evaluate_dataframe(\n",
        "        test,\n",
        "        pred_col=pred_col,\n",
        "        gold_col=GOLD_COL,\n",
        "        jaccard_threshold=0.6,\n",
        "        use_jaccard=True,\n",
        "        normalize_gold=True,\n",
        "    )\n",
        "\n",
        "    rows.append({\n",
        "        \"Setting\": pred_col,\n",
        "        \"micro_P\": metrics[\"micro\"][\"precision\"],\n",
        "        \"micro_R\": metrics[\"micro\"][\"recall\"],\n",
        "        \"micro_F1\": metrics[\"micro\"][\"f1\"],\n",
        "        \"tp\": metrics[\"micro\"][\"tp\"],\n",
        "        \"fp\": metrics[\"micro\"][\"fp\"],\n",
        "        \"fn\": metrics[\"micro\"][\"fn\"],\n",
        "        \"macro_F1\": metrics[\"macro\"][\"f1\"],\n",
        "        \"weighted_F1\": metrics[\"weighted_macro\"][\"f1\"],\n",
        "        \"exact_acc\": metrics[\"accuracy_exact\"],\n",
        "    })\n",
        "\n",
        "summary_df = pd.DataFrame(rows).sort_values(\"micro_F1\", ascending=False).reset_index(drop=True)\n",
        "summary_df\n"
      ],
