        "# --- A) POS-gated noun-only filter (Arabic) ---\n",
        "from typing import List\n",
        "# cache once\n",
        "_MLE = None\n",
        "_TOK = None\n",
        "def get_mle_tok():\n",
        "    global _MLE, _TOK\n",
        "    if _MLE is None:\n",
        "        from camel_tools.disambig.mle import MLEDisambiguator\n",
        "        from camel_tools.tokenizers.word import simple_word_tokenize\n",
        "        _MLE = MLEDisambiguator.pretrained()\n",
        "        _TOK = simple_word_tokenize\n",
        "    return _MLE, _TOK\n",
        "\n",
        "def keep_nouny_phrases_ar(phrases):\n",
        "    if not phrases: return []\n",
        "    try:\n",
        "        mle, tok = get_mle_tok()\n",
        "        out = []\n",
        "        for ph in phrases:\n",
        "            toks = tok(str(ph))\n",
        "            if not toks: continue\n",
        "            dis = mle.disambiguate(toks)\n",
        "            # keep if ANY token is noun/proper (camel tags usually start with 'n')\n",
        "            if any(((d.analyses[0].pos or d.analyses[0].pos_tag or '').lower().startswith('n'))\n",
        "                   for d in dis if d.analyses):\n",
        "                out.append(ph)\n",
        "        return out\n",
        "    except Exception:\n",
        "        return phrases\n",
        "\n",
        "\n",
        "# --- B) Morphological dedupe preferring verbatim-in-source & longest span ---\n",
        "def dedupe_keep_verbatim_longest(cands: List[str], source_text: str) -> List[str]:\n",
        "    \"\"\"\n",
        "    1) group by normalized key\n",
        "    2) within each group, prefer candidates that occur verbatim in the source (normalized match OK)\n",
        "    3) if multiple, keep the longest surface span\n",
        "    4) return unique list in stable order\n",
        "    \"\"\"\n",
        "    if not cands:\n",
        "        return []\n",
        "    src_norm = normalize_phrase(source_text or \"\")\n",
        "    groups = {}\n",
        "    for c in cands:\n",
        "        key = normalize_phrase(c)\n",
        "        if not key:\n",
        "            continue\n",
        "        groups.setdefault(key, []).append(c)\n",
        "\n",
        "    chosen = []\n",
        "    for key, spans in groups.items():\n",
        "        # prefer verbatim hits\n",
        "        verbatim = [s for s in spans if normalize_phrase(s) in src_norm]\n",
        "        pool = verbatim if verbatim else spans\n",
        "        # pick the longest\n",
        "        best = max(pool, key=lambda s: (len(normalize_phrase(s)), len(s)))\n",
        "        chosen.append(best)\n",
        "\n",
        "    # keep longest-non-substring set globally (avoid \"ﾘｮﾘｯﾙﾘｩ\" vs \"ﾘｮﾘｯﾙﾘｩ ﾘｧﾙﾘｺﾘｱﾙ―")\n",
        "    ordered = sorted(set(chosen), key=lambda x: (-len(normalize_phrase(x)), x))\n",
        "    kept = []\n",
        "    for a in ordered:\n",
        "        if not any((a != b) and (normalize_phrase(a) in normalize_phrase(b)) for b in kept):\n",
        "            kept.append(a)\n",
        "\n",
        "    # preserve original order where possible\n",
        "    final = [c for c in cands if c in kept]\n",
        "    for c in kept:\n",
        "        if c not in final:\n",
        "            final.append(c)\n",
        "    return final\n"
