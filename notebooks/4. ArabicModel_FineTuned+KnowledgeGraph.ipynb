      "source": [
        "# Prepare containers\n",
        "round_cols = {2: [], 3: [], 4: [], 5: []}\n",
        "\n",
        "for s in tqdm(test[\"sentence\"], desc=\"Running Arabic IG up to 5 rounds\"):\n",
        "    hist = iterative_residual_decoding_with_history_ar(s, max_rounds=5)\n",
        "\n",
        "    last = hist[-1] if hist else []\n",
        "\n",
        "    for r in [2, 3, 4, 5]:\n",
        "        if len(hist) >= r:\n",
        "            round_cols[r].append(hist[r-1])\n",
        "        else:\n",
        "            round_cols[r].append(last)\n",
        "\n",
        "# Assign to DataFrame\n",
        "for r in [2, 3, 4, 5]:\n",
        "    test[f\"ig_rounds_{r}\"] = round_cols[r]\n"
      ],
