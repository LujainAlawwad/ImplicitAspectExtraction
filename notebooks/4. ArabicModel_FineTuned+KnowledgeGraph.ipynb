        "# --- helpers ---\n",
        "from typing import List, Optional, Iterable\n",
        "\n",
        "def _as_iter(texts) -> Iterable[str]:\n",
        "    if texts is None:\n",
        "        return []\n",
        "    if isinstance(texts, (list, tuple)):\n",
        "        return [str(t) for t in texts if isinstance(t, (str, int, float))]\n",
        "    return [str(texts)]\n",
        "\n",
        "def _dedup_longest(spans: List[str]) -> List[str]:\n",
        "    \"\"\"Keep longest mentions; drop those that are substrings of others.\"\"\"\n",
        "    ordered = sorted(set(spans), key=lambda x: (-len(x), x))\n",
        "    kept = []\n",
        "    for a in ordered:\n",
        "        if not any((a != b) and (a in b) for b in kept):\n",
        "            kept.append(a)\n",
        "    # return in original order where possible\n",
        "    original = [s for s in spans if s in kept]\n",
        "    for s in kept:\n",
        "        if s not in original:\n",
        "            original.append(s)\n",
        "    return original\n",
        "\n",
        "def post_process_ar(\n",
        "    raw_pred: str,\n",
        "    source_text: Optional[str] = None,\n",
        "    paraphrases: Optional[Iterable[str]] = None,\n",
        "    camel_obj: Optional[CamelAdjRemover] = None,\n",
        "    enforce_mode: str = \"IG\",           # \"IG\" (source only) or \"PARA\" (source+paraphrases)\n",
        "    verbatim_mode: str = \"strict\",      # \"strict\" or \"normalized\"\n",
        "    # knobs:\n",
        "    enable_head_noun_fallback: bool = True,\n",
        "    para_min_votes: int = 0,            # 0(off), 1, 2...\n",
        "    kg_lexicon: Optional[set] = None,   # normalized KG aspects\n",
        "    min_chars: int = 2,\n",
        "    drop_stop_adjectives: bool = True,\n",
        "    nouns_only_after_camel: bool = False,  # (reserved for stricter POS filtering)\n",
        ") -> List[str]:\n",
